{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "RPN (with args)",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/RPN",
        "args": ["1 2 3 4 + * -"],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  